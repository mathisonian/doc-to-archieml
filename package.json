{
  "name": "@newswire/doc-to-archieml",
  "version": "0.0.0",
  "description": "Convert an ArchieML-formatted Google Doc directly into usable JSON using the Google Docs API.",
  "main": "index.js",
  "types": "types.d.ts",
  "files": [
    "index.js",
    "types.d.ts"
  ],
  "scripts": {
    "git-pre-commit": "precise-commits",
    "release": "np",
    "test": "mocha --timeout 3000 tests/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdmurphy/doc-to-archieml.git"
  },
  "keywords": [
    "googleapis",
    "archieml",
    "templating",
    "googledocs"
  ],
  "author": "Ryan Murphy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdmurphy/doc-to-archieml/issues"
  },
  "homepage": "https://github.com/rdmurphy/doc-to-archieml#readme",
  "dependencies": {
    "archieml": "^0.4.2",
    "googleapis": ">=37.1.0"
  },
  "devDependencies": {
    "@zeit/git-hooks": "^0.1.4",
    "mocha": "^6.0.2",
    "np": "*",
    "precise-commits": "^1.0.2",
    "prettier": "^1.16.4"
  },
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
